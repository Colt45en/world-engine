{
  "version": "0.2.0",
  "configurations": [
    {
        "name": "(Windows) Attach",
        "type": "cppvsdbg",
        "request": "attach",
        "processId": "${command:pickProcess}"
    },
    {
      "name": "Run Working Server (venv311)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/Documents/game101/Downloads/recovered_nucleus_eye/world-engine-feat-v3-1-advanced-math/working_python_server.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "python": "${workspaceFolder}\\.venv311\\Scripts\\python.exe"
    },
    {
      "name": "Run Smoke Test (venv311)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/tests/smoke_test_ws.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "python": "${workspaceFolder}\\.venv311\\Scripts\\python.exe"
    }
  ],
  "compounds": [
    {
      "name": "Server + Smoke Test",
      "configurations": ["Run Working Server (venv311)", "Run Smoke Test (venv311)"]
    }
  ]
}